{
  "compilerOptions": {
     "target": "ES2022",
    "module": "ESNext",
    "moduleResolution": "NodeNext",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "outDir": "dist",
    "baseUrl": "./src",
    "paths": {"@/*": ["*"],
    
    },
    "typeRoots": ["src/types", "./node_modules/@types"]
  },
   "include": ["src/**/*"],

}
