{
    "name": "backend",
    "version": "1.0.0",
    "main": "dist/src/index.js",
    "license": "MIT",
    "type": "module",
    "scripts": {
        "dev": "nodemon dist/src/index.js",
        "build": "tsc",
        "start": "node dist/src/index.js",
        "gen": "node gen-jwt-secret.js",
        "autogen": "node scripts/generateSecrets.js",
        "test": "echo \"Error: no test specified\" && exit 1",
        "seed": "npx tsx prisma/seed.ts",
        "seed1": "node --loader ts-node/esm prisma/seed.ts",
        "postinstall": "prisma generate && prisma migrate deploy",
        "db:reset": "npx prisma migrate reset",
        "db:generate": "npx prisma generate",
        "db:migrate": "npx prisma migrate dev",
        "db:deploy": "npx prisma migrate deploy",
        "db:push": "npx prisma db push",
        "studio": "npx prisma studio",
        "lint": "npx eslint .",
        "lint:fix": "npx eslint . --fix",
        "pretty": "npx prettier . --check",
        "pretty:fix": "npx prettier . --write",
        "format": "npm run pretty:fix && npm run lint:fix"
    },
    "dependencies": {
        "@prisma/adapter-mariadb": "^7.0.0",
        "@prisma/client": "^6.19.0",
        "@types/jsonwebtoken": "^9.0.10",
        "@types/speakeasy": "^2.0.10",
        "@typescript-eslint/eslint-plugin": "^8.46.3",
        "bcryptjs": "^3.0.2",
        "cookie-parser": "^1.4.7",
        "cors": "^2.8.5",
        "date-fns": "^4.1.0",
        "dotenv": "^16.6.1",
        "express": "^4.18.2",
        "express-fileupload": "^1.5.2",
        "helmet": "^8.1.0",
        "jsonwebtoken": "^9.0.3",
        "morgan": "^1.10.1",
        "nodemailer": "^7.0.10",
        "passport": "^0.7.0",
        "passport-google-oauth20": "^2.0.0",
        "qrcode": "^1.5.4",
        "speakeasy": "^2.0.0",
        "yup": "^1.7.1"
    },
    "devDependencies": {
        "@eslint/js": "^9.39.1",
        "@types/bcryptjs": "^2.4.6",
        "@types/cookie-parser": "^1.4.10",
        "@types/cors": "^2.8.19",
        "@types/date-fns": "^2.5.3",
        "@types/express": "^4.17.17",
        "@types/express-fileupload": "^1.5.1",
        "@types/morgan": "^1.9.10",
        "@types/node": "^20.19.24",
        "@types/nodemailer": "^7.0.3",
        "@types/passport": "^1.0.17",
        "@types/passport-google-oauth20": "^2.0.17",
        "@types/qrcode": "^1.5.6",
        "eslint": "^9.39.1",
        "eslint-plugin-react": "^7.37.5",
        "globals": "^16.5.0",
        "nodemon": "^3.1.10",
        "prisma": "^6.19.0",
        "ts-node": "^10.9.2",
        "ts-node-dev": "^2.0.0",
        "tsconfig-paths": "^4.2.0",
        "tsx": "^4.21.0",
        "typescript": "^5.9.3",
        "typescript-eslint": "^8.46.3"
    }
}
